<%= form_for @listing do |form| %>
	<%= form.label :name %><br/>
	<%= form.text_field :name %><br/>
	<%= form.label :place_type %><br/>
	<%= form.text_field :place_type %><br/>
	<%= form.label :property_type %><br/>
	<%= form.text_field :property_type %><br/>
	<%= form.label :room_number %><br/>
	<%= form.text_field :room_number %>	<br/>
	<%= form.label :bed_number %><br/>
	<%= form.text_field :bed_number %><br/>
	<%= form.label :guest_number %><br/>
	<%= form.text_field :guest_number %><br/>
	<%= form.label :country %><br/>
	<%= form.text_field :country %><br/>
	<%= form.label :state %><br/>
	<%= form.text_field :state %><br/>
	<%= form.label :city %><br/>
	<%= form.text_field :city %><br/>
	<%= form.label :zipcode %><br/>
	<%= form.text_field :zipcode %><br/>
	<%= form.label :address %><br/>
	<%= form.text_field :address %><br/>
	<%= form.label :price %><br/>
	<%= form.text_field :price %><br/>
	<%= form.label :description %> <br/> 
	<%= form.text_field :description %> <br/> 


    <%= form.label :images %><br/>
    <% @listing.images.each do |image| %>
    <%= image_tag(image.url) %>
    <% end %>
    <%= form.file_field :images, multiple: true%><br/>
    <%= form.hidden_field :images_cache %>

	<%= form.submit %>

<% end %>



